<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/visSearchImg">

    <ImageView
        android:id="@+id/visSearchImgImg"
        android:layout_width="234dp"
        android:layout_height="234dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.498"
        app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/visSearchImgPrompt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.171" />

    <Button
        android:id="@+id/visSearchImgNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <Button
        android:id="@+id/visSearchImgWord1"
        android:layout_width="94dp"
        android:layout_height="48dp"
        android:stateListAnimator="@null"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/visSearchImgImg"
        app:layout_constraintEnd_toEndOf="@+id/visSearchImgImg"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/visSearchImgImg"
        app:layout_constraintTop_toTopOf="@+id/visSearchImgImg"
        app:layout_constraintVertical_bias="0.715" />

    <Button
        android:id="@+id/visSearchImgWord2"
        android:layout_width="94dp"
        android:layout_height="48dp"
        android:stateListAnimator="@null"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/visSearchImgImg"
        app:layout_constraintEnd_toEndOf="@+id/visSearchImgImg"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/visSearchImgImg"
        app:layout_constraintTop_toTopOf="@+id/visSearchImgImg"
        app:layout_constraintVertical_bias="0.172" />

    <Button
        android:id="@+id/visSearchImgWord3"
        android:layout_width="94dp"
        android:layout_height="48dp"
        android:stateListAnimator="@null"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/visSearchImgImg"
        app:layout_constraintEnd_toEndOf="@+id/visSearchImgImg"
        app:layout_constraintHorizontal_bias="0.192"
        app:layout_constraintStart_toStartOf="@+id/visSearchImgImg"
        app:layout_constraintTop_toTopOf="@+id/visSearchImgImg"
        app:layout_constraintVertical_bias="0.381" />
</androidx.constraintlayout.widget.ConstraintLayout>